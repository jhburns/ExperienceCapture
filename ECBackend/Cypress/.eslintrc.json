{
    "extends": [
        "plugin:cypress/recommended",
        "eslint:recommended"
    ],
    "globals": {
        "module": true
    },
    "rules": {
        "cypress/require-data-selectors": "error",
        "jsx-quotes": "warn",
        "no-multiple-empty-lines": "warn",
        "no-process-env": "warn",
        "semi-style": [
            "warn",
            "last"
        ],
        "no-trailing-spaces": "warn",
        "indent": [
            "warn",
            2
        ],
        "semi": [
            "warn",
            "always"
        ]
    }
}